    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "1.7.3",
    "concurrently": "4.1.0",
    "express": "4.16.4",
    "express-http-proxy": "1.5.0",
    "lodash": "4.17.11",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
